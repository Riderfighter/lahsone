{
  "short_name": "lahsone",
  "name": "LAHSone",
  "description": "A dashboard for all things LAHS",
  "manifest_version": 2,
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "lahsone"
  },
  "version": "0.0.0.1",
  "permissions": [
    "*://mvla.asp.aeries.net/*",
    "*://bell.plus/*",
    "*://announcements.catapultcms.com/*",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  }
}
